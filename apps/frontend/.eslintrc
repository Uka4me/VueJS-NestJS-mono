{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "project": "./apps/frontend/tsconfig.json",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "@vue/typescript/recommended",
    "alloy",
    "alloy/vue",
    "alloy/typescript",
    "../../.eslintrc"
  ],
  "plugins": [],
  "rules": {
    "@typescript-eslint/prefer-optional-chain": "off",
    "no-console": "error",
    "no-debugger": "error"
  }
}
